{
  "name": "express-boilerplate",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "eslint": "./node_modules/.bin/eslint",
    "nodemon": "./node_modules/.bin/nodemon",
    "check": "npm run eslint -- ./",
    "serve": "npm run check && npm run nodemon -- -e .twig,.json,.js ./bin/index.js",
    "start": "npm run check && node ./bin/index.js"
  },
  "dependencies": {
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.26.0",
    "bcrypt": "^1.0.3",
    "body-parser": "~1.18.2",
    "connect-busboy": "0.0.2",
    "connect-mongo": "^2.0.0",
    "express": "~4.16.2",
    "express-session": "^1.15.5",
    "lodash": "^4.17.4",
    "mongoose": "^4.11.13",
    "morgan": "~1.9.0",
    "multer": "^1.3.0",
    "nodemailer": "^4.1.3",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "twig": "~1.10.5",
    "uniqid": "^4.1.1",
    "validator": "^9.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.1",
    "eslint": "^4.7.2",
    "nodemon": "^1.12.1"
  }
}
